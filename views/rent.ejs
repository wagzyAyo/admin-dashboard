<%- include('partials/header') %>
<h1><%=Data[0].tag%></h1>
<% Data.forEach(data =>{ %>
    <div class="box card">
        <div class="icons">
            <div class="space">
                <div>
                    <i class="fa-solid fa-pen"></i>
                </div>
                <div>
                    <i class="fa-solid fa-trash"></i>
                </div>
                
            </div>
        </div>
        
        <div class="intro">
            <b><p><%= data.name %>||</p></b>
            <p><%= data.location %>||</p>
            <p><%= data.size %>||</p>
            <p>#<%= data.amount %></p>
        </div>
        <div class="descr">
            <h4><%= data.short %></h4>
        </div>
    </div>
    
<% }) %>
<%- include('partials/footer') %>